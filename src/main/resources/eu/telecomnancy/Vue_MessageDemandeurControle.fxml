<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" fillHeight="false" spacing="50.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.telecomnancy.Vue_MessageDemandeurControle_Controller">
   <children>
      <TextArea fx:id="message" prefHeight="100.0" prefWidth="300.0" />
      <Button mnemonicParsing="false" onAction="#envoyer" prefHeight="38.0" prefWidth="134.0" text="Envoyer Message" wrapText="true" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>
