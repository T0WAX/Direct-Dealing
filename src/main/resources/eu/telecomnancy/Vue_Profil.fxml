<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" spacing="20.0" stylesheets="@Profil.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.telecomnancy.Vue_Profil_Controller">
   <children>
      <Label fx:id="titreLabel" text="Votre profil" textAlignment="CENTER">
         <font>
            <Font name="Ubuntu" size="30.0" />
         </font>
      </Label>
      <ImageView fx:id="profilImage" fitHeight="250.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="nomLabel" text="Nom">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
         <font>
            <Font name="Ubuntu" size="15.0" />
         </font>
      </Label>
      <Label fx:id="prenomLabel" text="Prenom">
         <font>
            <Font name="Ubuntu" size="15.0" />
         </font>
      </Label>
      <Label fx:id="descriptionLabel" text="Description" wrapText="true">
         <font>
            <Font name="Ubuntu" size="15.0" />
         </font>
      </Label>
      <Label fx:id="florainLabel" text="Nombre de florains" wrapText="true">
         <font>
            <Font name="Ubuntu" size="15.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
