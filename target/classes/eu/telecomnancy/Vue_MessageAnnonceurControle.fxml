<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_RIGHT" fillHeight="false" prefHeight="120.0" prefWidth="958.0" spacing="50.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.telecomnancy.Vue_MessageAnnonceurControle_Controller">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="457.0" spacing="20.0">
         <children>
            <TextArea fx:id="message" prefHeight="100.0" prefWidth="300.0" />
            <Button mnemonicParsing="false" onAction="#envoyer" prefHeight="40.0" prefWidth="139.0" text="Envoyer Message" wrapText="true" />
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="440.0" spacing="20.0">
         <children>
            <Button fx:id="accepter" mnemonicParsing="false" onAction="#accepter" text="Accepter Proposition" wrapText="true" />
            <Button fx:id="refuser" mnemonicParsing="false" onAction="#refuser" text="Refuser Proposition" wrapText="true" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
